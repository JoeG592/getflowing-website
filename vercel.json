{
  "buildCommand": "echo 'Static site'",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/pages/signup.html",
      "destination": "/signup.html"
    },
    {
      "source": "/pages/signup",
      "destination": "/signup.html"
    },
    {
      "source": "/pages/signin.html",
      "destination": "/signin.html"
    },
    {
      "source": "/pages/signin",
      "destination": "/signin.html"
    },
    {
      "source": "/pages/pricing.html",
      "destination": "/pricing.html"
    },
    {
      "source": "/pages/pricing",
      "destination": "/pricing.html"
    }
  ],
  "redirects": [
    {
      "source": "/dashboard",
      "destination": "/dashboard.html",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-Requested-With, Content-Type, Authorization"
        }
      ]
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  }
}
